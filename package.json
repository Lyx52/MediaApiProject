{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "proto": "protoc --es_opt=target=ts,import_extension=.ts --es_out=./src/proto --proto_path=./src/proto plugnmeet_recorder.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.2.0",
    "@bufbuild/protoc-gen-es": "^1.2.0",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "9.3.12",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.3.12",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/microservices": "^9.3.12",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "9.3.12",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/typeorm": "9.0.1",
    "@protobuf-ts/protoc": "^2.8.3",
    "bull": "^4.10.4",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "ioredis": "^5.3.1",
    "livekit-server-sdk": "^1.1.2",
    "mongodb": "3.6.0",
    "plugnmeet-sdk-js": "^1.2.2",
    "protobufjs": "^7.2.3",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.1",
    "rxjs": "7.5.5",
    "typeorm": "0.3.12",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.15.0-1",
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.12",
    "@types/cron": "^2.0.1",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
